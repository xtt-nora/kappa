{
  "name": "wellfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate-types && vite",
    "build:dev": "npm run generate-types && vite build --mode development",
    "build:test": "npm run generate-types && vite build --mode test",
    "build:pro": "npm run generate-types && vite build --mode production",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "preview": "npm run build:dev && vite preview",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "nvm": "nvm use 22",
    "generate-types": "node generate-types.js -- --configfile=./test-router-all.json"
  },
  "dependencies": {
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.5",
    "echarts": "^5.5.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "luckyexcel": "^1.0.1",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "screenfull": "^6.0.2",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.3.11",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.2.5",
    "vxe-pc-ui": "^4.0.3",
    "vxe-table": "^4.7.8",
    "xe-utils": "^3.5.18",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/qs": "^6.9.12",
    "@typescript-eslint/eslint-plugin": "~6.21.0",
    "@typescript-eslint/parser": "~6.21.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.0",
    "postcss": "^8.4.34",
    "postcss-html": "^1.6.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "prettier": "^3.2.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.3",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-html": "^3.2.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  }
}
